{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "prepare": "nuxt prepare",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "lint": "eslint .",
    "typecheck": "vue-tsc",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@nuxt/eslint": "catalog:",
    "@nuxt/icon": "catalog:",
    "@nuxtjs/color-mode": "catalog:",
    "@unocss/nuxt": "catalog:",
    "@vueuse/nuxt": "catalog:",
    "eslint": "catalog:dev",
    "monaco-editor": "catalog:",
    "monaco-themes": "catalog:",
    "monaco-vim": "catalog:",
    "nuxt": "catalog:",
    "splitpanes": "catalog:",
    "vue": "catalog:frontend",
    "vue-router": "catalog:frontend"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/utils": "catalog:",
    "@shikijs/markdown-it": "catalog:",
    "@shikijs/monaco": "catalog:",
    "@types/markdown-it": "catalog:",
    "@types/node": "catalog:dev",
    "@unocss/eslint-plugin": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "lint-staged": "catalog:dev",
    "markdown-it": "catalog:",
    "markdown-it-async": "catalog:",
    "shiki": "catalog:",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:build",
    "vitest": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
